{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "appointer/swaggervel": "^2.2",
        "barryvdh/laravel-cors": "^0.9.2",
        "brozot/laravel-fcm": "^1.2",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "~6.0",
        "infyomlabs/adminlte-templates": "5.5.x-dev",
        "infyomlabs/generator-builder": "dev-master",
        "infyomlabs/laravel-generator": "5.5.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/annotations": "^5",
        "laravelcollective/html": "^5.4",
        "league/fractal": "^0.17.0",
        "sentry/sentry-laravel": "^0.8.0",
        "yajra/laravel-datatables-buttons": "^3.0",
        "yajra/laravel-datatables-oracle": "~8.0",
        "zircote/swagger-php": "^2.0",
        "zizaco/entrust": "5.2.x-dev",
        "nesbot/carbon": "2.0.0-beta.4 as 1.25.0",
        "kylekatarnls/laravel-carbon-2": "^1.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravelcollective/html": "^5.5.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
