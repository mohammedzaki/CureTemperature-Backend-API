    "paths": {
        "/users/save-device-token/{userId}": {
            "put": {
                "tags": [
                    "User"
                ],
                "summary": "Store a newly created resource in storage.",
                "description": "This can only be done by the logged in user.",
                "operationId": "saveUserDeviceToken",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "deviceToken",
                        "in": "query",
                        "description": "mobile device token",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "deviceType",
                        "in": "query",
                        "description": "mobile device type",
                        "required": true,
                        "type": "string",
                        "enum": [
                            "android",
                            "ios",
                            "win"
                        ]
                    },
                    {
                        "$ref": "#/parameters/userId"
                    }
                ],
                "responses": {
                    "default": {
                        "$ref": "#/responses/SuccessResponse"
                    }
                }
            }
        },
        "/userDevices/{userId}": {
            "get": {
                "tags": [
                    "UserDevices"
                ],
                "operationId": "getUserDevices",
                "parameters": [
                    {
                        "$ref": "#/parameters/userId"
                    },
                    {
                        "name": "devicesId",
                        "in": "query",
                        "description": "List of devices IDs",
                        "required": false,
                        "type": "array",
                        "items": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "default": {
                        "$ref": "#/responses/device"
                    }
                }
            }
        },
        "/apiLogin": {
            "post": {
                "tags": [
                    "User"
                ],
                "operationId": "apiLogin",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "userLoginData",
                        "in": "body",
                        "description": "user Login Data",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/LoginData"
                        }
                    }
                ],
                "responses": {
                    "default": {
                        "$ref": "#/responses/SuccessResponse"
                    }
                }
            }
        }
    },
